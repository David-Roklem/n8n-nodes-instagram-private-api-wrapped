{
  "name": "n8n-nodes-instagram-private-api-wrapped-logo-changed",
  "version": "0.0.10",
  "description": "n8n community node for Instagram automation using private API capabilities",
  "license": "MIT",
  "homepage": "https://github.com/David-Roklem/n8n-nodes-instagram-private-api-wrapped-logo-changed",
  "author": {
    "name": "tiagohintz",
    "email": "tiagohintz@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/David-Roklem/n8n-nodes-instagram-private-api-wrapped-logo-changed.git"
  },
  "bugs": {
    "url": "https://github.com/David-Roklem/n8n-nodes-instagram-private-api-wrapped-logo-changed/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/nodes/InstagramCredentials.js"
    ],
    "nodes": [
      "dist/nodes/Instagram.node.js"
    ]
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "instagram",
    "social-media",
    "automation",
    "workflow",
    "instagram-private-api",
    "private-api",
    "private-api-wrapped"
  ],
  "files": [
    "dist/**/*",
    "package.json",
    "README.md",
    "LICENSE",
    "nodes/instagram.svg"
  ],
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "build": "tsc && npm run copy-assets",
    "copy-assets": "cp -r src/nodes/*.svg dist/nodes/ 2>/dev/null || true",
    "dev": "tsc --watch",
    "format": "prettier src --write",
    "lint": "eslint src/**/*.ts --max-warnings 3",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "dependencies": {
    "instagram-private-api": "^1.45.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^18.19.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "jest": "^29.6.4",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  }
}
